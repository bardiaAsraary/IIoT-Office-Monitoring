    {
        "id": "3fa35312d8884f0a",
        "type": "function",
        "z": "e692544aa3fd13e6",
        "name": "Parse WAMP Messages",
        "func": "// Bulletproof WAMP Parser - handles ALL common formats\ntry {\n    const raw = msg.payload;\n\n    // Case 1: Standard WAMP format [16, id, details, topic, data]\n    if (Array.isArray(raw) && raw.length >= 5 && raw[0] === 16) {\n        return {\n            payload: {\n                temperature: raw[4].temp || raw[4].temperature || 0,\n                humidity: raw[4].hum || raw[4].humidity || 0,\n                timestamp: new Date().toLocaleTimeString()\n            }\n        };\n    }\n    // Case 2: Simple {topic, payload} format\n    else if (raw.topic) {\n        return {\n            payload: {\n                temperature: raw.payload.temp || raw.payload.temperature || 0,\n                humidity: raw.payload.hum || raw.payload.humidity || 0,\n                timestamp: new Date().toLocaleTimeString()\n            }\n        };\n    }\n    // Case 3: Direct sensor data (fallback)\n    else {\n        return {\n            payload: {\n                temperature: raw.temp || raw.temperature || 0,\n                humidity: raw.hum || raw.humidity || 0,\n                timestamp: new Date().toLocaleTimeString()\n            }\n        };\n    }\n} catch (e) {\n    node.error(\"Parser error - invalid format:\", { error: e, payload: raw });\n    return null;\n}",
        "outputs": 1,
        "timeout": "",
        "noerr": 1,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 510,
        "y": 180,
        "wires": [
